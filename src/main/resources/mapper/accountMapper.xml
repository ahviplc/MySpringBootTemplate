<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lc.myMapper.MyAccountMapper">

    <!-- selectByName -->
    <select id="selectByName" resultType="com.lc.myEntity.TbAccount">
        select *
        from tb_account
        where user_name = #{name}
    </select>

</mapper>
